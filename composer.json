{
  "name": "3xw/cakephp-newsletter",
  "description": "CakePHP newsletter plugin",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.5.9",
    "cakephp/cakephp": "^3.3",
    "dereuromark/cakephp-queue": "3.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Trois\\Newsletter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\Newsletter\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
